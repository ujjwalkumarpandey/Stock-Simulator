    <nav id="navBar">
        <img id="navLogo" src="/images/logo2.png" alt="logo">
        <h2>Trade Vantage</h2>
        <div class="navBarList">
            <a href="/api/showUserStocks/stockHome">Home</a>
            <a href="/api/showUserStocks/showUserStocks">My Stocks</a>
            <a href="/api/showUserStocks/stockSelect">Stocks</a>
            <a href="/api/profileauth/transactionHistory">Transaction History</a>
        </div>
    </nav>


    <div>
        <h1 style="display: flex; justify-content:center; align-items:center;">All Stocks</h1>
    </div>

    <div class="container">
        <ul class="responsive-table">
            <li class="table-header">
                <div class="col col-2"><b>Name</b></div>
                <div class="col col-2"><b>Current Price</b></div>
                <div class="col col-3"><b>Change</b></div>
                <div class="col col-4"><b>52W High</b></div>
                <div class="col col-5"><b>52W Low</b></div>
            </li>

            {{#each resultWithProfit}}
            <a class="insideBtn"
                href="/api/showUserStocks/productDescription?id={{this.symbol}}&identifier='{{this.identifier}}'&open={{this.open}}&dayHigh={{this.dayHigh}}&dayLow={{this.dayLow}}&lastPrice={{this.lastPrice}}&previousClose={{this.previousClose}}&change={{this.change}}&pChange={{this.pChange}}&totalTradedVolume={{this.totalTradedVolume}}&totalTradedValue={{this.totalTradedValue}}&lastUpdateTime={{this.lastUpdateTime}}&yearHigh={{this.yearHigh}}&yearLow={{this.yearLow}}&perChange365d={{this.perChange365d}}&perChange30d={{this.perChange30d}}">
                <li class="table-row">
                    <div class="col col-2" data-label="Customer Name" style="color:#342110">{{this.identifier}} </div>
                    <div class="col col-2" data-label="Amount" style="color:#342110"> ₹{{this.lastPrice}}</div>
                    {{#if flagProfit}}
                    <div class="col col-3" data-label="Amount" style="color:Green;">
                        {{this.change}}<br>({{this.pChange}}%)</div>
                    {{else}}

                    <div class="col col-3" data-label="Amount" style="color:red;">{{this.change}}<br>({{this.pChange}}%)
                    </div>
                    {{/if}}

                    <div class="col col-4" data-label="Payment Status" style="color:#342110">₹{{this.yearHigh}}</div>
                    <div class="col col-5" data-label="Payment Status" style="color:#342110">₹{{this.yearLow}}</div>
                </li>
                {{!-- <a id="autoBuyBtn" href="/api/sell/autoBuy?id={{this.identifier}}">Auto Buy</a> --}}
            </a>
            {{/each}}
        </ul>
    </div>





